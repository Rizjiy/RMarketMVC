@model RMarket.WebUI.Models.TesterModel

@{
    ViewBag.Title = "Index";
}
@using (Html.BeginForm("BeginTest","Emul",FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{
    @Html.ValidationSummary()
    <fieldset>
        <div class="form-group">
            @Html.LabelFor(m => m.InstanceId, new { @class = "col-sm-2" })
            <div class="col-sm-10">
                @Html.DropDownListFor(m => m.InstanceId, ViewBag.InstanceList as IEnumerable<SelectListItem>, "--SELECT--", new { @class = "form-control" })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(m => m.ConnectorInfoId, new { @class = "col-sm-2" })
            <div class="col-sm-10">
                @Html.DropDownListFor(m => m.ConnectorInfoId, ViewBag.ConnectorInfoList as IEnumerable<SelectListItem>, "--SELECT--", new { @class = "form-control" })
            </div>
        </div>
    </fieldset>
        
    <div class="panel-footer">
        <input type="submit" value="Поехали" class="btn btn-primary" />
        @Html.ActionLink("Отмена", "Index", null, new { @class = "btn btn-default" })
    </div>
}
